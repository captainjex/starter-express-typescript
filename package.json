{
  "name": "choolifah-school-web",
  "version": "1.0.0",
  "description": "",
  "main": "dist/runner.js",
  "scripts": {
    "prebuild": "npm run lint:fix",
    "build": "rimraf dist/* && tsc",
    "build:all": "concurrently -p \"[{name}]\" -n \"TypeScript,LaravelMix\" -c \"cyan.bold,blue,\" \"npm run build\" \"npm run mix:prod\"",
    "dev": "ts-node-dev src/runner.ts",
    "start": "node .",
    "mix:dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "mix:prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --profile --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "migration:generate": "node script/migration-generate.js",
    "migrate": "npm run build && sequelize db:migrate --env development",
    "migrate:undo": "sequelize db:migrate:undo --env development",
    "migrate:reset": "sequelize db:migrate:undo:all --env development",
    "seed": "npm run build && sequelize db:seed:all --env development",
    "seed:generate": "node script/seed-generate.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.41",
    "@types/express": "^4.17.6",
    "@types/express-flash": "0.0.2",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mjml": "^4.0.4",
    "@types/node": "^14.0.14",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/pug": "^2.0.4",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.1.0",
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^7.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.50",
    "typescript": "^3.9.2",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "alpinejs": "^2.5.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.6.3",
    "moment": "^2.27.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.10",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.10",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.1.0",
    "ui-huma": "file:packages/ui-huma",
    "vue": "^2.6.11",
    "winston": "^3.3.3"
  }
}
